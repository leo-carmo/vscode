        }
      },
      {
        "command": "editor.action.replaceAll",
        "args": {
          "find": " Para",
          "replace": " para",
          "isRegex": true
        }
      },
      {
        "command": "editor.action.replaceAll",
        "args": {
          "find": " Tanto Como",
          "replace": " tanto como",
          "isRegex": true
        }
      }
    ]
  }
